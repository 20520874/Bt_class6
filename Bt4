#include <iostream>
#include <math.h>
using namespace std;
#define MAX 100

void NhapmangSNT(int a[], int &n);
int SoPhanTuChuaY(int a[], int n,int y);

bool KiemTraSNT(int a)
  {
      if (n==0||n==1) return 0;
     long i;
        for (i=2; i<=sqrt(n); i++)
        if(n%i==0) return 0;
    return 1;
  }

void NhapmangSNT(int a, int &n)
{
    cin >> n;
    int i=2, k=0;
    while (k<n){
        if (KiemTraSNT(i)=true) a[k++]=i;
        i++;
    }
}

