    }

int SoPhanTuChuaY(int a[], int n, int y)
{
    int dem = 0;
    for (int i=0; i<n-1; i++)
    if Kiemtrachuay(a[i], y)==true) dem=dem+1;
    return dem;
}

int main()
{
	int a[MAX], n, y;
	cin >>y;
	NhapmangSNT(a,n);
